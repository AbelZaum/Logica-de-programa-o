package exercicioaritmeticos4;

import java.util.Scanner;

public class ExercicioAritmeticos4 {

    public static void main(String[] args) {

       Scanner sc = new Scanner (System.in);
      
        float num1 = 4f;
        float num2 = 5f;
        float num3 = 6f;
        float num4 = 7f;
        float num5 = 8f;
        float num6 = 10f;

        float media1, media2;
        float mediasoma, media_media;

        media1 = (num2 + num1 + num6) / 3;
        media2 = (num5 + num4 + num3) / 3;

        mediasoma = media1 + media2;
        media_media = (media1 + media2) / 2;

        System.out.println("A media Aritmetica dos numeros: " + media1);
        System.out.println("A media dos numeros 8, 7 e 6 sao: " + media2);
        System.out.println("A soma das duas medias e: " + mediasoma);
        System.out.println("A media das medias e: " + media_media);   
    
    sc.close();
    
    }
    
}

