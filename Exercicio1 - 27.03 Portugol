programa
{
	
	funcao inicio()
	{
		
		inteiro numero1, numero2
		
		escreva ("Informe o numero 1: ")
		leia (numero1)

		escreva ("Informe o numero 2: ")
		leia (numero2)

		se ( numero1 == numero2) {
			escreva ("Resultado = Igual")

		}senao {
				escreva ("Resultado = Diferente")
		}
						
	}
}
