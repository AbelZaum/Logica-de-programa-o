programa
{
	
	funcao inicio()
	{

    		inteiro numero1, numero2, numero3

    		escreva ("Informe o numero 1: ")
    		leia (numero1)

    		escreva ("Informe o numero 2: ")
    		leia (numero2)

    		escreva ("Informe o numero 3: ")
    		leia (numero3)

    			se ((numero1 == numero2) e (numero2 == numero3)){
    		
    		    		escreva ("Resultado = Iguais")
    		   
    			} senao {
    				escreva ("Resultado = Diferentes")  	
    			
    			}
	}
}
